{
  "extends": [
    "airbnb",
    "react-app"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "mocha": true,
    "jest": true
  },
  "rules": {
    "no-trailing-spaces": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/forbid-prop-types": 0,
    "jsx-a11y/href-no-hash": "off"
  }
}
